<?xml version="1.0"?>
<schema>
	<element name="address">
	  <complexType>
	    <attribute name="kind"  use="optional">
		  <simpleType>
		    <restriction base="string">
			  <enumeration value="street"/>
			  <enumeration value="postcode"/>
			  <enumeration value="phone"/>
			  <enumeration value="email"/>
			  <enumeration value="url"/>	  			  			  			  			  			  
		    </restriction>
		</simpleType>	 
	</attribute>
  </complexType>
</element>
</schema>